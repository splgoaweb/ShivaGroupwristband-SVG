                    var imgURI = canvas
                        .toDataURL('image/png')
                        .replace('image/png', 'image/octet-stream');
                    console.log('imgURI : ', imgURI);

                    triggerDownload(imgURI);
                };
            } catch (error) { console.log('error > ',error); }
            img.src = url;
        })


